package vadim;
class GFG { 
	  
    // Funcție pentru verificarea șirului alfabetic
    public static boolean isStringOnlyAlphabet(String str) 
    { 
        return ((str != null) 
                && (!str.equals("")) 
                && (str.matches("^[a-zA-Z]*$"))); 
    } 
  
    // Maainul
    public static void main(String[] args) 
    { 
        System.out.println("Test Case 1:"); 
        String str1 = "Paralelogram"; 
        System.out.println("Input: " + str1); 
        System.out.println( "Output: "+ isStringOnlyAlphabet(str1)); 
  

        System.out.println("\nTest Case 2:"); 
        String str2 = "Paralelorgramul cu 4 laturi"; 
        System.out.println("Input: " + str2); 
        System.out.println("Output: "+ isStringOnlyAlphabet(str2)); 
  
 
        System.out.println("\nTest Case 3:"); 
        String str3 = null; 
        System.out.println("Input: " + str3); 
        System.out.println( "Output: "+ isStringOnlyAlphabet(str3)); 
   
    } 
} 
